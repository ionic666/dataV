    function cbqkDiv(myMum){//轮播及前后切换主方法
        if(myMum==0){
            $("#wx1").show();
            $("#wx1").siblings().hide();
            $('#jxMap1').show();
            $('.jxMap').not("#jxMap1").hide();
        }else if(myMum==1){
            $("#wx2").show();
            $("#wx2").siblings().hide();
            $('#jxMap3').show();
            $('.jxMap').not("#jxMap3").hide();
        }else if(myMum==2){
