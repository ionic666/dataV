		var myChart = echarts.init(document.getElementById(id));
		var option = {
			tooltip: {
				show: false
			},
			color:['#FAFF75','#3176A9'],
			grid: {
				x: '10%',
				y: '25%',
				x2: '3%',
				y2: '16%',
				containLabel: true,
				borderWidth: 0
			},
			xAxis : [
				{
					show : false,
					splitLine: {show: false},
					type : 'category',
					data : ["2017"]
				}
			],
			yAxis : [
				{
					show : false,
					splitLine: {show: false},
					type : 'value'
