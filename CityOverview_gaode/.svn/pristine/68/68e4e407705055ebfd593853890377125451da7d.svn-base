    <script>
        var map = new AMap.Map('map', {
            mapStyle: 'amap://styles/16e4e3ccd1f66f229a6276fe149db8df',//样式URL
            zoom: 12
        });
        var headerWidth = $("._header").width();	//获取头部标题的宽度
        $("._header").css("left", -headerWidth / 2 - 115 + "px");//让头部标题居中

        //页面初始化数据
        $(document).ready(function () {
            pageInit();
        });
        function pageInit() {
            $.ajax({
                type: "POST",
                url: '../../jxecokey/findkeycomdata.do',
                dataType: 'json',
                async: false,
                success: function (data) {
                    var bhRange = data.bhRange;//区域获取
                    var menuList = data.first;//导航栏获取
                    var mapcom = data.mapcom;//地图显示企业获取
                    var keycom = data.keycom;//表格全部企业获取获取
                    var bhRangeHtml = "",
                            menuListHtml = "",
                            keycomHtml = "";
                    $(".brief-a").show();
                    $(".brief-a>p").text(menuList[0].des);
                    //添加html---开始
                   /* for (var i = 0; i < bhRange.length; i++) {
                        if (i == 0) {
                            bhRangeHtml += '<option value="" selected="selected">嘉兴全市</option>';
                            bhRangeHtml += '<option value=' + bhRange[i].bhValue + '>' + bhRange[i].mc + '</option>';
                        } else {
                            bhRangeHtml += '<option value=' + bhRange[i].bhValue + '>' + bhRange[i].mc + '</option>';
                        }
                    }*/
                    for (var i = 0; i < menuList.length; i++) {
                        if (i == 0) {
                            menuListHtml += '<li class="active" data-id=' + menuList[i].id + '>' + menuList[i].name + '</li><ul class="tel tel'+menuList[i].id+'"></ul>';
                            $(".menuTitle").text(menuList[i].name);
//                            $("#echarts-1").text(menuList[i].des);
                        } else {
                            menuListHtml += '<li data-id=' + menuList[i].id + '>' + menuList[i].name + '</li><ul  class="tel  tel'+menuList[i].id+'"></ul>';
                        }
                    }
                  /*  for (var i = 0; i < keycom.length; i++) {
                        var num = i + 1;
                        keycomHtml += '<tr data-name='+keycom[i].NAME+'><td width="20%">' + num + '</td><td width="80%">' + keycom[i].NAME + '</td> </tr>';
                    }*/
                    $("#bhRange").html(bhRangeHtml);
                    $("#menuList").html(menuListHtml);
                    $("#menuList").children("li:last").click()
                   /* $("#industrials #tableBody").html(keycomHtml);*/

                    for (var i = 0; i < mapcom.length; i++) {
                        var jwd = [mapcom[i].JD, mapcom[i].WD];//经纬度纠偏

                        addPoint(jwd, 30, 30, mapcom[i]);
                    }
                    $(".divqy").css('display', 'block');
                    $(".gdxx").html(mapcom[0].SHAREHOLDER_INFO);
                    $(".flss").html(mapcom[0].LAWSUIT);
                    $(".zpxx").html(mapcom[0].RECRUITMENT_INFO);
                    $(".ztb").html(mapcom[0].BID);
                    $(".zzzs").html(mapcom[0].QUALIFICATION);
                    $(".sbxx").html(mapcom[0].TRADEMARK_INFO);
                    $(".zlxx").html(mapcom[0].PATENT_INFO);
                    $(".zzq").html(mapcom[0].COPYRIGHT);
                    ////添加地图信息---结束
                    //echarts数据添加--开始
//                echarts1Show(data.valueList);
//                    echarts2Show(data.ageList);
                    //echarts数据添加--结束

                }
            });
        }
        //页面跳转
        $("#pageJump").on("change", function () {
            if ($(this).val() == "hxqy") {
                location.href = "hlwjjqs.jsp";
            }
        })
        //导航栏点击事件
        $("#menuList").on("click", "li", function () {
            map.clearMap();
            var id = $(this).attr('data-id'),
                    bh = $('#bhRange').val();

            $(this).siblings().removeClass('active');
            $("#industrials #tableBody").html("");
            $(this).addClass('active');
            $(this).siblings('ul').css("display","none");
            zdqyAjax(id, bh, 002,$(this));
        })
        function searchEnter() {
            //回车的key为13,如果是回车就调用搜索方法
            if (event.keyCode == 13) {
                search();
            }
        }
        //搜索框
        function search() {
            var searchValue = $("#search").val();
            map.clearMap();
            map.clearInfoWindow();
            $.ajax({
                type: "POST",
                url: '../../jxecokey/serchKeycomName.do',
                dataType: 'json',
                async: false,
                data: {name: searchValue},
                success: function (data) {
                    //添加地图信息---开始
                    for (var i = 0; i < data.length; i++) {
                        var jwd = [data[i].JD, data[i].WD];//经纬度纠偏
                        addPoint(jwd, 30, 30, data[i]);
                    }
                    infoWindow(data[0]);
                    ////添加地图信息---结束

                }
            })
        }
        $("#menuList").delegate(".qyName",'click',function(){
//            $("#search").val($(this).text());
//            search();
//            $("#search").val("");
            infoWindow($(this).data("data"));
            map.setCenter([parseFloat($(this).data("data").JD),parseFloat($(this).data("data").WD)]);
        })

        function zdqyAjax(id, bh, state,dom) {
//            console.log(id+':'+bh)
            $.ajax({
                type: "POST",
                url: '../../jxecokey/queryComByCondition.do',
                dataType: 'json',
                async: false,
                data: {id: id, bhValue: bh},
                success: function (data) {
                    if(id==1){
                        $(".brief-b").hide();
                        $(".brief-a").show();
                        $(".brief-a>p").html('<span style="font-size:20px;color:#0CDAF5">中国电子百强：</span>'+data.first[0].des);
                    }else if(id==2){
                        $(".brief-b").hide();
                        $(".brief-a").show();
                        $(".brief-a>p").html('<span style="font-size:20px;color:#0CDAF5">省级百强：</span>'+data.first[0].des);
                    }else if(id==4){
                        $(".brief-a").hide();
                        $(".brief-b").show();
                        echartsPie(data.pie);
                    }else{
                        $(".brief-b").hide();
                        $(".brief-a").show();
                        $(".brief-a>p").text(dom.text()+data.first[0].des);
                    }
                    var first = data.first;
                    var mapcom = data.mapcom;//地图显示企业获取
                    var keycom = data.keycom;//表格全部企业获取获取
                    var p='';
                    var bhRangeHtml = "",
