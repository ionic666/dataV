        ////关闭信息窗体
        function closeInfoWindow() {
            map.clearInfoWindow();
            $(".gdxx").html("");
            $(".flss").html("");
            $(".zpxx").html("");
            $(".ztb").html("");
            $(".zzzs").html("");
            $(".sbxx").html("");
            $(".zlxx").html("");
            $(".zzq").html("");
        }
        //实例化信息窗体
        //构建自定义信息窗体
        function createInfoWindow(title, content) {
            var info = document.createElement("div");
            info.className = "info";

            //可以通过下面的方式修改自定义窗体的宽高
            info.style.width = "360px";
            // 定义顶部标题
            var top = document.createElement("div");
            var titleD = document.createElement("div");
            var closeX = document.createElement("img");
            top.className = "info-top";
            titleD.innerHTML = title;
            closeX.src = "../../images/gaode-close.png";
            closeX.onclick = closeInfoWindow;

            top.appendChild(titleD);
            top.appendChild(closeX);
            info.appendChild(top);

            // 定义中部内容
            var middle = document.createElement("div");
            middle.className = "info-middle";
            middle.style.backgroundColor = 'white';
            middle.innerHTML = content;
            info.appendChild(middle);

            // 定义底部内容   //下方的小箭头
            var bottom = document.createElement("div");
            bottom.className = "info-bottom";
            bottom.style.position = 'relative';
            bottom.style.top = '0px';
            bottom.style.margin = '0 auto';
            var sharp = document.createElement("img");
            sharp.src = "../../images/hlw-sj.png";
            bottom.appendChild(sharp);
            info.appendChild(bottom);
            return info;
        }
        function infoWindow(details) {
//  console.log(details)
            if (details) {

                var title = '<span style="font-size:11px;color:#0CDAF5;">' + details.NAME + '</span>';
                content = [];

                content.push("<span class=left-content>地址</span><span class='right-content'>"+details.ADDR+"</span>");
//			content.push("<div class=infoContentDiv><span class='infoName'>联系方式</span><p><span>"+details.TEL+"</span></p></div>");
                content.push("<span class=left-content>公司类型</span><span class='right-content'>"+details.TYPE+"</span>");

                content.push("<span class=left-content>注册资本</span><span class='right-content'>"+details.REGISTERED_CAPITAL+"万元</span>");
                content.push("<span class=left-content>注册时间</span><span class='right-content'>"+details.CREATEDATE+"</span>");
                content.push("<span class=left-content>法人</span><span class='right-content'>"+details.BUSINESS_ENTITY+"</span>");
//			content.push("<div class=infoContentDiv><span class='infoName'>企业年产值</span><p><span>"+details.VALUE+"亿元</span></p></div>");
                content.push("<span class=left-content>经营范围</span><span class='right-content'>"+details.SCOPE+"</span>");
                var infoWindow = new AMap.InfoWindow({
                    isCustom: true,  //使用自定义窗体
                    content: createInfoWindow(title, content.join("")),
                    offset: new AMap.Pixel(10, -30)
                });
                var jw = [details.JD, details.WD];
                map.setCenter(jw);
                //setTimeout(function(){
                infoWindow.open(map, jw);
                //},500)
            }
        }
        function addPoint(arr, h, w, details) {//arr =[经度,纬度]；h=图片高度，w=图片宽	度；img=图片路径
            var _marker = new AMap.Marker({
                map: map,
                position: arr,        //经纬度
                icon: new AMap.Icon({
                    size: new AMap.Size(h, w),  //图标大小
                    image: "../../images/qy.png"       //自定义图片
                })
            });
            AMap.event.addListener(_marker, 'click', function (e) {
                infoWindow(details)
                $(".divqy").css('display', 'block');
                $(".gdxx").html(details.SHAREHOLDER_INFO);
                $(".flss").html(details.LAWSUIT);
                $(".zpxx").html(details.RECRUITMENT_INFO);
                $(".ztb").html(details.BID);
                $(".zzzs").html(details.QUALIFICATION);
                $(".sbxx").html(details.TRADEMARK_INFO);
                $(".zlxx").html(details.PATENT_INFO);
                $(".zzq").html(details.COPYRIGHT);
                //infoWindow.open(map, _marker.getPosition());
            });
        }
