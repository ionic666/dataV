*{
    box-sizing: border-box;
    font-size: 16px;
}
html,body,ul,li{
    padding:0;
    margin:0;
}
html,body{
    width: 100%;
    height: 100%;
}
li{
    list-style: none;
}
a:hover{
    text-decoration: none;
}
/*头部系统设置*/
header{
    width: 100%;
    height: 9%;
    background-color: #21292B;
    padding: 20px 0 10px 20px;
}
header img{
    vertical-align: middle;
}
span.setting{
    font-size: 40px;
    font-weight: bold;
    color: #0CDAF5;
    vertical-align: middle;
}
#GoBack{
    background-color: #21292B;
    color: #21292B;
    position: absolute;
    right: 3%;
    top: 3%;
}
#GoBack:hover{
    cursor: pointer;
    color: #fff
}
/*主体*/
div#main{
    padding-top: 1%;
    height: 91%;
    width: 100%;
    background-color: #34403E;
}
div#left{
    width: 10%;
    height: 100%;
    float: left;
}
/*导航*/
div#mainNav{
    width: 100%;
    height: 100%;
    background-color: #21292B;
    float: left;
}
div#mainNav ul#managerNav{
    width: 100%;
    height: 100%;
    padding-top: 10%;
}
div#mainNav ul#managerNav img{
    vertical-align: middle;
}
div#mainNav ul#managerNav span{
    vertical-align: middle;
    color: #0CDAF5;
    font-size: 18px;
}
div#mainNav ul#managerNav li{
    margin-bottom: 8%;
    width: 100%;
    padding-left: 15%;
    height: 40px;
    line-height: 40px;
    font-weight: bold;
    cursor: pointer;
    border-left: 2px solid transparent;
}
div#mainNav ul#managerNav li.active{
    background-color: #3C4343;
    border-left: 2px solid #0CDAF5;
    cursor: pointer;
}
/*嵌入页面*/
div#right{
    margin-left: 1%;
    width: 89%;
    height: 100%;
    float: left;
    position: relative;
}
div#right iframe{
    width: 100%;
    height: 100%;
}