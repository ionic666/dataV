            $("#wx5").siblings().hide();
            $('#jxMap6').show();
            $('.jxMap').not("#jxMap6").hide();
        }
    }
    $("#lunboTimeImg1>img").click(function(){//开始暂停自动轮播
        var timeNum=parseInt($(".shbzPress-active").attr('data-num'));
        if(lunboTimeImg1State==false){

            lunboTimeImg1State=true;
            $("#lunboTimeImg1>img").attr('src',"images/shbz-play1.png");
            IntervalShbz=setInterval(function(){
                if(timeNum>=4){
                    timeNum=-1;
                }
                $(".shbzPress-circle").eq(timeNum+1).addClass("shbzPress-active");
                $(".shbzPress-circle").eq(timeNum+1).siblings().removeClass("shbzPress-active");
                cbqkDiv(timeNum+1);
                timeNum++;
            },2000);
        }else{
            clearInterval(IntervalShbz);//清除定时器
//            IntervalShbz=null;
            timeNum=$(".shbzPress-active").attr('data-num');
            lunboTimeImg1State=false;
            $("#lunboTimeImg1>img").attr('src',"images/shbz-stop1.png");
        }
    }).mouseover(function(){

        if(lunboTimeImg1State==false){
            $("#lunboTimeImg1>img").attr('src',"images/shbz-stop2.png");

        }else{

            $("#lunboTimeImg1>img").attr('src',"images/shbz-play2.png");

        }
    }).mouseout(function (){
        if(lunboTimeImg1State==false){
            $("#lunboTimeImg1>img").attr('src',"images/shbz-stop1.png");
        }else{
            $("#lunboTimeImg1>img").attr('src',"images/shbz-play1.png");
        }
    });
    $("#lunboTimeImg2>.next").click(function(){//下一步
        var activeNum;
        activeNum=parseInt($(".shbzPress-active").attr('data-num'));
        clearTime();

        if(activeNum>=4){
            activeNum=-1;
        }
        cbqkDiv(activeNum+1);//右侧信息框

        $(".shbzPress-circle").eq(activeNum+1).addClass("shbzPress-active");
        $(".shbzPress-circle").eq(activeNum).removeClass("shbzPress-active");

        activeNum++;

        $("#lunboTimeImg2>.next").attr('src',"images/shbz-prev1.png");
    }).mouseover(function(){
        $("#lunboTimeImg2>.next").attr('src',"images/shbz-prev2.png");
    }).mouseout(function (){
        $("#lunboTimeImg2>.next").attr('src',"images/shbz-prev1.png");
