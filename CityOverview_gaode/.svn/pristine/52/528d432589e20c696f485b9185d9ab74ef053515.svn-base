        clearTime();
        if(activeNum<=-1){
            activeNum=4;

        }

        $(".shbzPress-circle").eq(activeNum-1).addClass("shbzPress-active");
        $(".shbzPress-circle").eq(activeNum).removeClass("shbzPress-active");
        if(activeNum==0){
            cbqkDiv(4);//右侧信息框
        }else{
            cbqkDiv(activeNum-1);//右侧信息框
        }
        activeNum--;

        $("#lunboTimeImg2>.prev").attr('src',"images/shbz-next1.png");
    }).mouseover(function(){
