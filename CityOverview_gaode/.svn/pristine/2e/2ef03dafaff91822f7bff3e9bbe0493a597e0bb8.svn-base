    xxhModalEchart: function (data) {
    /*    var font_color =  '#fff', font_family = '微软雅黑', font_size = 7;
        var font_color_y = '#BFBFBF';
        var text_style={
            color:font_color,
            fontSize:font_size,
            fontFamily:font_family
        };*/
        var mycharts = echarts.init(document.getElementById('xxhModalEcharts'));
        var option = {
            tooltip : {
                trigger: 'axis'
            },
            calculable : false,

            legend: {
                data:['工业总产值','增速'],
                textStyle: {fontSize: 15, color:'#fff'},
                x: 'center',
                y: 'top'
            },
            grid: {
                x: '10%',
                y: '20%',
                x2: '10%',
                y2: '15%',
                containLabel: true,
                borderWidth: 0
            },
            xAxis : [
                {
                    type : 'category',
                    axisTick: {show: false, lineStyle: {color: font_color}},
                    axisLine: {show: true, lineStyle: {color: font_color}},
                    axisLabel: {show: true, textStyle: text_style},
                    splitLine: {show: false},
                    data : data.years
                }
            ],
            yAxis : [
                {
                    type : 'value',
                    name : '产值(元）',
                    axisLine: {show: true,lineStyle: {color: font_color}},
                    axisLabel: {show: false, textStyle: text_style},
                    axisTick: {show: false, lineStyle: {color: font_color}},
                    splitLine: {
                        show: false,
                        lineStyle: {color: font_color}
                    }
                    /*axisLabel : {
                     formatter: '{value} '
                     }*/
                },
                {
                    type : 'value',
                    name : '增速（%）',
                    axisLine: {show: true,lineStyle: {color: font_color}},
                    axisLabel: {show: false, textStyle: text_style},
                    axisTick: {show: false, lineStyle: {color: font_color}},
                    splitLine: {
                        show: false,
                        lineStyle: {color: font_color}
                    }
                    /*axisLabel : {
                     formatter: '{value} %'
                     }*/
                }
            ],
            series : [
                {
                    name: '工业总产值',
                    type:'bar',
                    barWidth: 100,
                    itemStyle: {
                        normal: {
                            color: function(params) {
                                // build a color map as your need.
                                var colorList = [
                                    '#FFE600','#FF7600','#02ABC2'
                                ];
                                return colorList[params.dataIndex]
                            }

                        }
                    },
                    data:data.industrytotal
                },
                {
                    name: '增速',
                    type:'line',
                    yAxisIndex: 1,
                    itemStyle:{
                        normal:{
                            label:{
                                show:true,
                                position:'inside',
                                formatter: '{c}%',
                                textStyle:{
                                    color: '#fff'
                                }
                            },
                            lineStyle:{
                                shadowColor:'green',
                                shadowBlur: 40
                            }
                        }
                    },
                    data:data.industryspeed
                }
            ]
        };



        mycharts.setOption(option);
        mycharts.on('click',function(e){
            industryModal.tableAjax(e.name);
        })
    },
    tableAjax:function(year){
        var tr=$("<tr>");
        var title=["一级指标","二级指标","三级指标","数值"]
        for(var i=0;i<title.length;i++){
            var td=$("<td>");
            tr.append(td.html(title[i]));
        }
        $(".tableBodyBox table thead").html(tr)
        $.ajax({
            type: 'post',
            url: "jxecocore/queryCoreData.do",
            data:{year:year},
            dataType: 'JSON',
            success: function (data) {
                var len=data.length;
                firstRowSpanArr=[]
                for(var i=0;i<len;i++){
                    var firstRowSpanNo=0;
                    var slen=data[i].statistics.length;
                    for(var n=0;n<slen;n++){
                        for(var m=0;m<2;m++) {
                            firstRowSpanNo++;
                        }
                    }
                    firstRowSpanArr.push(firstRowSpanNo)
                }
                var str=""
                for(var i=0;i<len;i++){
                    var lnn=data[i].statistics.length;
                    str+=' <tr> <td rowspan="'+firstRowSpanArr[i]+'">'+data[i].mc+'</td>'
                    var str1="";
                    for(var n=0;n<lnn;n++){
                        var lnn2=data[i].statistics[n].third.length;
                        str1+='<td rowspan="2">'+data[i].statistics[n].secondmc+'</td>'
                        var str2="";
                        for(var m=0;m<2;m++){
                            if(m==0){
                                str2+= '<td>'+'工业总产值增速'+'</td>'+ '<td>'+data[i].statistics[n].third[0].industryspeed+'%</td>'
                            }else if(m==1){
                                str2+= '<td>'+'利润总额增速'+'</td>'+ '<td>'+data[i].statistics[n].third[0].profitspeed+'%</td>'
                            }
                            str2+='</tr>';

                        }
                        str1+=str2
                    }
                    str+=str1
                }
                $(".tableBodyBox table tbody").html(str);
                //设置表格隔行变色；
                $(".tableBodyBox table tr:even").css("background-color",'#303030');
                $(".tableBodyBox table tr:odd").css("background-color",'#1B1B1D');
                cloneTableHead()//克隆表头，并让表头固定
            }
        })
    }
};

