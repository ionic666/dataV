                    fontSize: 18
                },
                series: [
                    {
                        name: '访问来源',
                        type: 'pie',
                        radius: ['40%', '55%'],
                        itemStyle:{
                            normal: {
                                label: {
                                    show: true,
                                    formatter: '{b}\n({c})',
                                }
                            }
                        },
                        label: {
