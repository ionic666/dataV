* {
    box-sizing: border-box !important;
}

ul {
    margin: 0;
    padding: 0;
    list-style: none;
}

html, body {
    padding: 0;
    margin: 0;
    height: 100%;
    background: url('../../images/adminbg.png') no-repeat center;
    overflow: hidden;
}

.container {
    width: 98%;
    height: 100%;
    margin: 0 1%;
}

.top {
    height: 10%;
    text-align: center;
    position: relative;
}

.return-btn {
    position: absolute;
    top: 9%;
    right: 0;
    cursor: pointer;
}

.title-btn {
    margin-top: 1.3%;
}

.middle {
    height: 90%;
}

.middle-left, .middle-right {
    width: 25%;
    float: left;
    height: 100%;
}

.middle-middle {
    width: 48%;
    float: left;
    height: 100%;
    margin: 0 1%;
}

.middle-left-1, .middle-right-1 {
    width: 100%;
    height: 60%;
    margin-bottom: 10px;
    border-top: 2px solid #0CDAF5;
    background: rgba(18,24,24,0.4);
    position: relative;
}

.middle-left-1 > span:nth-child(1), .middle-right-1 > span:nth-child(1) {
    position: absolute;
    top: -39px;
    left: 10px;
    display: inline-block;
    color: #fff;
    font-size: 24px;
}

.middle-left-1 > span:nth-child(2), .middle-right-1 > span:nth-child(2) {
    position: absolute;
    top: 10px;
    left: 10px;
    display: inline-block;
    color: #fff;
    font-size: 18px;
}


/*左侧工具条*/
.middle-left-1 > div:nth-of-type(1) {
    text-align: center;
    margin-top: 70px;
    height: 45px;
    position: relative;
    padding:0 40px;
}

.middle-left-1 > div:nth-of-type(1) span.man{
    width:19px;
    height:100%;
    background: url(../../images/nan.png) no-repeat center;
    float:left;
    display: block;
}

.middle-left-1 > div:nth-of-type(1) span.woman{
    width:19px;
    height:100%;
    background: url(../../images/nv.png) no-repeat center;
    float:left;
    display: block;
}

.middle-left-1 > div:nth-of-type(1) div.tool{
    width:calc(100% - 38px);
    width:-moz-calc(100% - 38px);
    height:100%;
    float:left;
}


.middle-left-1 > div:nth-of-type(1) div.tool .tool-man{
    width:60%;
    height:100%;
    float:left;
    background: url(../../images/m-1.png) repeat-x;
}

.middle-left-1 > div:nth-of-type(1) div.tool .tool-woman{
    width:40%;
    height:100%;
    float:left;
    background: url(../../images/m-2.png) repeat-x;
}


/*右侧工具条*/
.middle-right-1 > div:nth-of-type(1) {
    text-align: center;
    margin-top: 70px;
    height: 45px;
    position: relative;
    padding:0 40px;
}

.middle-right-1 > div:nth-of-type(1) span.man{
    width:19px;
    height:100%;
    background: url(../../images/nan.png) no-repeat center;
    float:left;
    display: block;
}

.middle-right-1 > div:nth-of-type(1) span.woman{
    width:19px;
    height:100%;
    background: url(../../images/nv.png) no-repeat center;
    float:left;
    display: block;
}

.middle-right-1 > div:nth-of-type(1) div.tool{
    width:calc(100% - 38px);
    width:-moz-calc(100% - 38px);
    height:100%;
    float:left;
}


.middle-right-1 > div:nth-of-type(1) div.tool .tool-man{
    width:60%;
    height:100%;
    float:left;
    background: url(../../images/m-3.png) repeat-x;
}

.middle-right-1 > div:nth-of-type(1) div.tool .tool-woman{
    width:40%;
    height:100%;
    float:left;
    background: url(../../images/m-4.png) repeat-x;
}





.middle-left-2 > div:nth-of-type(1) {
    text-align: center;
    margin-top: 60px;
    height: 40px;
}

.middle-left-2 > div:nth-of-type(1) span {
    display: inline-block;
    float: left;
    width: 50%;
    height: 100%;
    color: #fff;
    text-align: center;
    padding: 11px 0;
    font-size: 18px;
    font-weight: bolder;
}

.middle-right-2 > div:nth-of-type(1) {
    text-align: center;
    margin-top: 60px;
    height: 40px;
}

.middle-right-2 > div:nth-of-type(1) span:nth-child(1) {
    display: inline-block;
    float: left;
    width: 30%;
    height: 100%;
    color: #fff;
    text-align: center;
    padding: 11px 0;
    font-size: 18px;
    font-weight: bolder;
}

.middle-right-2 > div:nth-of-type(1) span:nth-child(2) {
    display: inline-block;
    float: left;
    width: 70%;
    height: 100%;
    color: #fff;
    text-align: center;
    padding: 11px 0;
    font-size: 18px;
    font-weight: bolder;
}

#echarts-1 {
    width: 100%;
    height: calc(100% - 115px);
    height: -moz-calc(100% - 115px);
}

#echarts-4 {
    width: 100%;
    height: calc(100% - 115px);
    height: -moz-calc(100% - 115px);
}

.middle-left-2, .middle-right-2 {
    width: 100%;
    height: calc(40% - 10px);
    height: -moz-calc(40% - 10px);
    border-top: 2px solid #0CDAF5;
    background: rgba(18,24,24,0.4);
    position: relative;
}


.middle-left-2 > span, .middle-right-2 > span {
    position: absolute;
    top: 10px;
    left: 10px;
    display: inline-block;
    color: #fff;
    font-size: 18px;
}

