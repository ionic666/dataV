.main{
    background:url("../images/tools/2.png") no-repeat;
    background-size: 100% 100%;
    width:83px;height:0px;
    overflow: hidden;
}
.main>div span{
    font-size: 12px;
    height: 30px;
    line-height: 30px;
    display: block;
    color: #6e6e6e;
}
.main>div{height:32px;line-height: 32px;border-bottom: 1px solid #dadada}

.ico{width:18px;height:18px;float: left;background-repeat: no-repeat;margin-left: 10px;margin-top: 8px;margin-right: 12px}
.ico_1{background-image: url(../images/tools/8.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_1_1{background-image: url(../images/tools/8-.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_2{background-image: url(../images/tools/3.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_2_2{background-image: url(../images/tools/3-.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_3{background-image: url(../images/tools/4.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_3_3{background-image: url(../images/tools/4-.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_4{background-image: url(../images/tools/5.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_4_4{background-image: url(../images/tools/5-.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_5{background-image: url(../images/tools/6.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_5_5{background-image: url(../images/tools/6-.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_6{background-image: url(../images/tools/7.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_6_6{background-image: url(../images/tools/7-.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_7{background-image: url(../images/tools/9.png);background-size: 100%,100%;background-repeat: no-repeat;}
.ico_7_7{background-image: url(../images/tools/9-.png);background-size: 100%,100%;background-repeat: no-repeat;}

.lead{float:left;margin-left:30px;margin-top:20px;}
.pie{width:70px;height:70px;background: url("../images/left/bg.png");position: absolute}
.home1{width:21px;height:22px;background: url("../images/left/home.png");position: relative;top:24px;left:25px;}
.up{width:10px;height:8px;background: url("../images/left/up.png");position: relative;top:-15px;left:30px;}
.right{width:8px;height:10px;background: url("../images/left/right.png");position: relative;top:1px;left:54px;}
.down{width:10px;height:8px;background: url("../images/left/down.png");position: relative;top:13px;left:30px;}
.left{width:8px;height:10px;background: url("../images/left/left.png");position: relative;top:-17px;left:8px;}
.right,.left,.up,.down,.home1{cursor: pointer;}
.up:hover{width:10px;height:8px;background: url("../images/left/up_arrow_hover.png");position: relative;top:-15px;left:30px;}
.right:hover{width:8px;height:10px;background: url("../images/left/right_arrow_hover.png");position: relative;top:1px;left:54px;}
.down:hover{width:10px;height:8px;background: url("../images/left/down_arrow_hover.png");position: relative;top:13px;left:30px;}
.left:hover{width:8px;height:10px;background: url("../images/left/left_arrow_hover.png");position: relative;top:-17px;left:8px;}

#change{
    width:206px;
    height:42px;
    position:absolute;
    z-index:666;
    top:8px;
    right:100px;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}
#change.imgping{background-image: url("../images/tools/1-2.png");}
#change.imgwei{background-image: url("../images/tools/2-2.png");}
#change.imgsan{background-image: url("../images/tools/3-2.png");}
#change div{
    float:left;
    height: 42px;
    width: 68px;
}
.ping{padding-left: 16px;padding-right:8px;}
.wei{padding-left:8px;padding-right:8px;}
.san{padding-left:8px;padding-right:20px;}
#change a{ text-decoration: none;color:#6e6e6e;  }
#change>.ping>a{color:#3A81FF;}
#change a:hover{  color:#3A81FF;  }