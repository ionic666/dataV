//	养老保险参保趋势
	function echarts0Show(arr) {
		var mycharts = echarts.init(document.getElementById('headEchart0'));
		var option = {
			color: ['#3696D9'],
			tooltip : {
				trigger: 'axis',
				/*采用回调函数*/
				formatter: function (params) {
					if(params.length == 0){
						return null;
					}
					else{
						var str=params[0].name+"<br />";
						for(var i = 0; i< params.length; i++){
							str+=params[i].seriesName+"："+params[i].value+"万人<br/>";
						}
					}
					return str;
				}
			},
			grid: {
				x: '12%',
				y: '20%',
				x2: '5%',
				y2: '15%',
				containLabel: true,
				borderWidth: 0
			},
			legend: {
				y:20,
				data:['参保人数'],
				textStyle: text_style,
				formatter: function (params) {
					return params+'(万人)'
				 }
			},
			calculable : false,
			xAxis : [
				{
					type : 'category',
					boundaryGap: ['20%', '20%'],
					axisLine: {lineStyle: {color: '#BFBFBF'}},
					splitLine: {show: false},
					axisLabel: {show: true, textStyle: text_style},
					axisTick: {show: false},
					data : ['2013年','2014年','2015年','2016年','2017年']
				}
			],
			yAxis : [
				{
					type: 'value',
//					min:0,
//					max:100,
					splitNumber:4,
					splitLine: {
						show: true,
						lineStyle:{
							color: ['#8D8C8C'],
							width: 1,
							type: 'dotted'
						}
					},
					axisLine: { lineStyle: {
						color: '#8D8C8C'
					}},
					axisTick: {show: false},
					axisLabel: {
						show: true,
						textStyle: text_style,
						formatter: '{value} '}
				}
			],
			series : [{
				 name:'参保人数',
				 type:'line',
				 data:arr,
				 itemStyle:{
					 normal:{
						 lineStyle:{
							 color:'#3696D9',
							 shadowColor:'#FFC000',
							 shadowBlur:40
						 }
					 }
				 }
			 }]
		};
		mycharts.setOption(option);
	}
//	养老保险年龄结构统计
	function echarts1Show(arr){
		var myChart=echarts.init(document.getElementById("headEchart1"));
		var option={
			color: ['#00B9B9'],
			tooltip: {
				trigger: 'axis',
				axisPointer: {
					type: 'shadow'
				},
				formatter: function (params) {
					if(params.length == 0){
						return null;
					}
					else{
						var str=params[0].name+"<br />";
						for(var i = 0; i< params.length; i++){
							str+=params[i].seriesName+"："+params[i].value+"人<br/>";
						}
					}
					return str;
				}
			},
			grid: {
				x: '5%',
				y: '15%',
				x2: '5%',
				y2: '5%',
				containLabel: true,
				borderWidth: 0
			},
			xAxis: {
				type: 'value',
				splitLine: {show: false},
				axisLine: {show: false},
				axisTick: {show: false},
				axisLabel: {show: false}
			},
			yAxis: {
				type: 'category',
				axisLine: {lineStyle: {color: '#676767'}},
				axisTick: {show: true},
				axisLabel: {show: true, textStyle: text_style, interval: 0},
				splitLine: {show: false},
				nameLocation: 'end',
				data: ['16-30岁','30-50岁','50-65岁','65岁以上']
			},
			series: [
				{
					name:'缴费人数',
					type: 'bar',
					itemStyle: {normal: {borderWidth: 0,label:{show:true,textStyle: text_style}}},
					barWidth: 18,
					barGap: '2%',
					data:arr
				}
			]
		};
		myChart.setOption(option);
	}
//	养老保险性别比例统计
	function echarts2Show(obj) {
		var myChart_rqnl = echarts.init(document.getElementById("headEchart2"));
		var option_rqnl = {
			color: ['#3696D9', '#00B9B9'],
			calculable: false,
//			legend: {
//				data: ['异地就医(女)', '异地就医(男)'],
//				x: 'center',
//				y: 'bottom',
//				itemGap: 40,
//				textStyle: {
//					color: '#fff'
//				}
//			},
			grid: {
			    x: '1%',
				x2: '5%',
				y: '15%',
                y2: '10%',
				containLabel: true,
				borderWidth: 0
				
			},
			tooltip: {
				trigger: 'axis',
				formatter: function (params, ticket, callback) {
					var res = '人群年龄分布(' + params[0].name + ')';
					for (var i = 0, l = params.length; i < l; i++) {
						if (params[i].value < 0) {
							params[i].value = -params[i].value;
						}
						res += '<br/>' + params[i].seriesName + ' : ' + params[i].value + '%';
					}
					setTimeout(function () {
						// 仅为了模拟异步回调
						callback(ticket, res);
					}, 100)
				}
			},
			xAxis: [
				{
					type: 'value',
					max: 30,
					min: -30,
					splitLine: {show: false},
					axisLine: {show: false},
					axisTick: {show: false},
					axisLabel: {
						show: true,
						textStyle: text_style,
						formatter: function (value) {
							if (value < 0) {
								return (-value + "%");
							} else {
								return (value + '%');
							}
						}
					}
				}
			],
			yAxis: [
				{
					type: 'category',
					splitLine: {show: false},
					axisLine: {show: false},
					axisLabel: {show: true, textStyle: text_style},
					axisTick: {show: false},
					data: ['16-30岁', '30-50岁', '50-65岁', '65岁以上']
				}
			],
			series: [

				{
					name: '异地就医(男)',
					type: 'bar',
					stack: '总量',
					barWidth: 15,
					itemStyle: {
						normal: {
							label: {
								show: false
							}
						}
					},
					data: obj.male
				},
				{
					name: '异地就医(女)',
					type: 'bar',
					stack: '总量',
					barWidth: 15,
					data: obj.female
				}
			]
		};
		myChart_rqnl.setOption(option_rqnl);
	}
