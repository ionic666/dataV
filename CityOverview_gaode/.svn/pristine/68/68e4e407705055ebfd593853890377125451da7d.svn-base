        function echartsPie(data) {
            var myChart = echarts.init(document.getElementById('echarts-1'));
            option = {
                color: ['#3D6B95', '#477CAB', '#5089BC', '#5795CD', '#72A5D8', '#97B9E0', '#B2C9E6', '#CAD9ED'],
                tooltip: {
                    trigger: 'item',
                    formatter: "{b}"
                },
