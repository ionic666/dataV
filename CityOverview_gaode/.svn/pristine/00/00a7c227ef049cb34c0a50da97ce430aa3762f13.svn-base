.middle-middle-1 {
    width: 100%;
    height: 60%;
    margin-bottom: 10px;
    background: #121818;
    position: relative;
}

.middle-middle-1 .ts-map{
    position: absolute;
    top:10px;
    left:30px;
    color:#fff;
    font-size: 18px;
}

.middle-middle-1 .ts-map div{
    margin:5px 5px;
    vertical-align: middle;
}

.middle-middle-1 .ts-map div:nth-child(1){
    color:#0CDAF5;
    font-size: 18px;
}

.middle-middle-1 .ts-map div:nth-child(2) i{
    width:18px;
    height:18px;
    background: darkgreen;
    border-radius: 50%;
    display: inline-block;
    margin:0 5px;
}

.middle-middle-1 .ts-map div:nth-child(3) i{
    width:18px;
    height:18px;
    background: #fff;
    border-radius: 50%;
    display: inline-block;
    margin:0 5px;
}

.middle-middle-2 {
    width: 100%;
