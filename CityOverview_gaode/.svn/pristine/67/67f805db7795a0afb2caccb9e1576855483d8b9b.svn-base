<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>阳光医保二级页面</title>
  <link rel="stylesheet" href="http://cache.amap.com/lbs/static/main1119.css"/>
  <link rel="stylesheet" href="../../myCss/headTitle.css"/>
  <style>
    body{
      width: 100%;
      height: 100%;
      background:url(" ../../images/ygyb-bg.png")no-repeat;
      background-size:100% 100%;
    }
    *{
      margin: 0;
      padding: 0;
      font-size: 18px;
    }
    li{
      list-style: none;
    }
    .main-left,.main-right{
      width: 49%;
      height: 85%;
      display: inline-block;
      margin: 6% 0 0 1%;
      float: left;
    }
    .main-left{
      background: #000;
    }
    .main-left>div{
      width: 100%;
    }
    .main-right>div{
      width: 98%;
      margin-left: 1%;
    }
    .main-left-a{
      position: relative;
      height: 10%;
      margin-bottom: 2%;
      background: #161719;
      border-top: 1px solid #077285;
      border-bottom: 1px solid #27292A;
    }
    .main-left-b{
      height: 90%;
      background: #161719;
      border: 1px solid #0CDAF4;
    }
    .main-right-a{
      height: 43%;

      margin-bottom: 2%;
      background: rgba(2,11,22,0.7) !important;
      border-top: 1px solid #0CDAF4;
    }
    .main-right-b{
      height: 55%;
      margin-bottom: 2%;
      background: rgba(2,11,22,0.8);
      border-top: 1px solid #0CDAF4;
    }
    .main-left-a>ul{
      width: 60%;
      /*height: 1.6em;*/
      position: absolute;
      top: 50%;
      margin-top: -0.65em;
    }
    #searchInfo{
      width: 45%;
      height: auto;
      position: absolute;
      color: #fff;
      background: rgba(0,0,0,0.7);
      top: 50%;
      margin-top: 0.85em;
      z-index: 800;
      display: none;
    }
    #searchInfo>ul>li{
      font-size: 0.8em;
      padding: 0.2em 1em;
      overflow: hidden;
      text-overflow:ellipsis;
      white-space: nowrap;
      cursor: pointer;
    }
    #searchInfo>ul>li:hover{
      color: #0CDAF5;
      background-color: #10485D;
    }
    .main-left-a>ul>li{
      width: 20%;
      color: #fff;
      float: left;
    }
    .main-left-a>ul span{
      font-size: 0.9em;
      padding: 0.2em;
      display: block;
      text-align: right;
    }
    .main-left-a>ul input{
      width: 100%;
      font-size: 0.9em;
      padding: 0.2em;
    }
    .main-left-a>ul button{
      color: #fff;
      font-size: 0.9em;
      border: 1px solid #CC7A29;
      border-radius: 0.2em;
      margin-left: 1em;
      padding: 0.2em 1.2em;
      background: #CC7A29;
      cursor: pointer;
    }
    .li-input{
      width: 45% !important;
    }
    .li-input>input{
      color: #fff;
      border:1px solid #272A2D;
      background: #161718 !important;
    }
    .main-right-a>h3,.main-right-b>h3{
      width: 98%;
      height: 10%;
      padding-top: 0.5em;
      margin-left: 2%;
      color: #fff;
    }
    .main-right-a>div{
      width: 100%;
      height: 90%;
    }
    .main-right-b-div-1{
      width: 70%;
      height: 20%;
      margin-left: 15%;
    }
    .main-right-b-div-2{
      width: 100%;
      height: 70%;
    }
    /*高德地图样式表*/
    .amap-toolbar{
      display:none;
    }
    .info {
      border: solid 1px #889DA2;
      border-radius: 2px;
      box-shadow: 0px 0px 8px #60d8ef;
      background: #001D24;
    }
    div.info-top {
      position: relative;
      background-color:rgba(0,29,36,0.8);
      border-bottom: 1px solid #7A9095;
      border-radius: 5px 5px 0 0;
    }
    div.info-top div {
      display: inline-block;
      color: #333333;
      font-size: 14px;
      font-weight: bold;
      line-height: 31px;
      padding: 0 10px;
    }
    div.info-top img {
      position: absolute;
      top: 10px;
      right: 10px;
      transition-duration: 0.25s;
      cursor: pointer;
    }
    div.info-top img:hover {
      box-shadow: 0px 0px 5px #60d8ef;
    }
    div.info-middle {
      font-size: 14px;
      padding: 6px;
      line-height: 20px;
      /*background-color:rgba(0,29,36,0.8) !important;*/
      background: #001D24 !important;
    }
    div.info-bottom {
      height: 0px;
      width: 100%;
      clear: both;
      text-align: center;
    }
    div.info-bottom img {
      position: relative;
      z-index: 104;
    }
    .info-top span,.info-middle span{
      margin-left: 5px;
      font-size: 18px !important;
    }
    .info-middle img {
      float: left;
      margin-right: 6px;
    }
    .amap-icon img{
      width:100%;
      height:100%;
    }

    #industrials table{
      width: 100%;
      height: 100%;
      text-align: left;
    }
    #tableBody tr{
      /*height: 40px;*/
    }
    .info-middle>div{
      position: relative;
      overflow: hidden;
      padding: 23px 1px 7px 38px;
    }
    .info-middle .left-content {
      float: left;
      width: 35%;
      display: inline-block;
      text-align: left;
      color: #32DDE7;
      margin: 8px 0;
      font-size: 18px;
    }
    .info-middle .right-content {
      float: left;
      width: 65%;
      display: inline-block;
      text-align: left;
      color: #fff;
      margin: 8px 0;
      font-size: 18px;
    }
    /*高德地图样式结束*/
    .main-right-b-div-1>div{
      float: left;
      height: 100%;
    }
    .main-right-b-div-1>.icon{
      width: 5%;
      float: left;
    }
    .main-right-b-div-1>.progress{
      width: 100%;
      height: 2.4em;
    }
    #man{
      width: 56%;
      background: #CC9949;
    }
    #women{
      width: 44%;
      background: #00A6A6;
    }
  </style>
</head>
<body>
<a href="#" onclick="returnMainPage()" style="max-width:3%;position: fixed;left:97%;top: 2%;z-index: 999"><img src="../../images/returnUp.png" alt=""></a>
<div class="_header">
  <img src="../../images/btz.png" alt=""/>
  <h1>企业参保情况</h1>  <%-- 主标题--%>
  <%--<span>智慧结算</span>--%>  <%--副标题--%>
  <img src="../../images/bty.png" alt=""/>
</div>
  <div class="main main-left">
    <div class="main-left-a" id="searchArea">
      <ul>
        <li><span>企业检索</span></li>
        <li class="li-input" class="search">
          <input type="text" placeholder="企业检索" id="searchQY"/>
          <div id="searchInfo">
          <ul>
          </ul>
          </div>
        </li>
        <li><button type="button" class="search" id="searchQYBtn">搜&nbsp;索</button></li>
      </ul>
      <%--<div id="searchInfo">--%>
        <%--<ul>--%>
          <%--<li>11111</li>--%>
          <%--<li>22222</li>--%>
          <%--<li>33333</li>--%>
          <%--<li>44444</li>--%>
        <%--</ul>--%>
      <%--</div>--%>
    </div>
    <div class="main-left-b" id="map">

    </div>
  </div>
  <div class="main main-right">
    <div class="main-right-a">
      <h3>企业平均参保金额和平均参保年龄</h3>
      <div id="echarts1"></div>
    </div>
    <div class="main-right-b">
      <h3>企业平均参保金额和平均参保年龄</h3>
      <div class="main-right-b-div-1">

        <div class="progress">

          <div style="width: 50%;height: 100%;float: left">

            <span id="man" style="height: 100%;float: right"></span>
            <img class="peopleIcon manIcon" src="../../images/cityPeopleCard/nan.png" alt=""  style="height: 100%;float: right;margin-right: 1em"/>
          </div>
          <div style="width: 50%;height: 100%;float: left">
            <span id="women" style="height: 100%;float: left"></span>
            <img class="peopleIcon womanIcon" src="../../images/cityPeopleCard/nv.png" alt="" style="height: 100%;float: left;margin-left: 1em"/>
          </div>

        </div>

      </div>
      <div class="main-right-b-div-2" id="echarts2"></div>
    </div>
  </div>

  <script type="text/javascript" src="http://webapi.amap.com/maps?v=1.4.1&key=966008e8aaafb827acaa2f1bea731a60"></script>
  <script src="//webapi.amap.com/ui/1.0/main.js?v=1.0.11"></script>
  <%--<script type="text/javascript" src="http://cache.amap.com/lbs/static/addToolbar.js"></script>--%>

  <script type="text/javascript" src="../../myjs/jquery-2.1.4.js"></script>
  <script type="text/javascript" src="../../map/js/bootstrap.js"></script>
  <script type="text/javascript" src="../../map/js/echarts-all.js"></script>
  <script type="text/javascript" src="../../myjs/index.js"></script>
  <script type="text/javascript" src="../../myjs/jquery.dataStatistics.js"></script>
  <script src="../../myjs/myEcharts.js"></script>
  <script>
    var headerWidth=$("._header").width();	//获取头部标题的宽度
    $("._header").css("left",-headerWidth/2+"px");//让头部标题居中
    var map = new AMap.Map('map', {
      mapStyle: 'amap://styles/16e4e3ccd1f66f229a6276fe149db8df',//样式URL
      zoom: 10
    });
    //搜索---开始
    function searchQY(name){
      $("#searchInfo").show();

      $.ajax({
        type: 'post',
        url: "../../jxSocialSecurity/serchSocailSecurityInfo.do",
        dataType: 'JSON',
        data:{name:name},
        success: function (data) {
//          console.log(data)
          var jwd = [data[0].JD,data[0].WD];//经纬度纠偏
          console.log(jwd)
          addPoint(jwd,30,30,data[0]);
          $("#searchInfo ul").html("");
          for(var i=0;i<data.length;i++){
            $("#searchInfo ul").append('<li title='+data[i].DWMC+'>'+data[i].DWMC+'</li>');
          }
          }

      })
    }
    $("#searchInfo ul").delegate('li','click',function(){
      $("#searchInfo").hide();
      var newName=$(this).text();
//      console.log(newName)
      $.ajax({
        type: 'post',
        url: "../../jxSocialSecurity/serchSocailSecurityInfo.do",
        dataType: 'JSON',
        data:{name:newName},
        success: function (data) {
//          console.log(data)
          var jwd = [data[0].JD,data[0].WD];//经纬度纠偏
//          console.log(jwd)
          addPoint(jwd,30,30);
        }

      })
    })
    $(document).bind('click', function(e) {
      var e = e || window.event; //浏览器兼容性
      var elem = e.target || e.srcElement;
      while (elem) { //循环判断至跟节点，防止点击的是div子元素
        if (elem.id && elem.id == 'searchArea') {
          return;
        }
        elem = elem.parentNode;
      }
      $("#searchInfo").hide(); //
    });
    $("#searchQYBtn").on('click',function(){
//      console.log(666)
      var name=$("#searchQY").val();
      searchQY(name);
    })
    //搜索---结束
//    搜索框change事件---开始
    $("#searchQY").bind('input','change',function(){
      console.log(666)
      var name=$(this).val();
      searchQY(name);
    })
    //搜索框change事件---结束

    //echarts--开始
    function echartsQY1(){
      var mycharts1 = echarts.init(document.getElementById('echarts1'));
      var option1 = {
        color: ['#13BFBF','#CC7A29'],
        tooltip : {
          trigger: 'axis',
          formatter:'{b}<br/>{a}:{c}万元<br/>{a1}:{c1}'
        },
        grid: {
          x: '15%',
          y: '10%',
          x2: '15%',
          y2: '20%',
          containLabel: true,
          borderWidth: 0
        },
        legend: {
          data:['平均参保金额','平均参保年龄'],
          show: true,
          y:'bottom',
          textStyle:{
            color:'#fff'
          },
        },
        xAxis : [
          {
            type : 'category',
            splitLine:false,
            axisLine: { lineStyle: {
              color: '#8D8C8C'
            }},
            axisTick: {show:false},
            axisLabel: {show: true, textStyle: text_style},
            data : ['2014年','2015年','2016年','2017年']
          }
        ],
        yAxis : [
          {
            type : 'value',
            axisLine: { lineStyle: {
              color: '#8D8C8C'
            }},
            splitNumber:3,
            splitLine: {
              show: true,
              lineStyle:{
                color: ['#8D8C8C'],
                width: 1,
                type: 'dashed'}},
            axisLabel: {
              show: true,
              textStyle: text_style,
              formatter: '{value} '
            }
          },
          {
//				name:'医保使用率',
//				nameTextStyle:text_style,
            type : 'value',
            axisLine: { lineStyle: {
              color: '#8D8C8C'
            }},
            splitLine:{show:false},
//				axisTick: {show: true,lineStyle:{color: '#bfbfbf'}},
            axisLabel: {
              show: true,
              textStyle: text_style,
              formatter: '{value} '}
          }
        ],
        series : [
          {
            name:'平均参保金额',
            type:'bar',
            barWidth:30,
            data:[600,700,900,1300]
          },
          {
            name:'平均参保年龄',
            type:'line',
            symbol:'rectangle',
            yAxisIndex: 1,
            data:[15,18,20,32]
          }
        ]
      };
      mycharts1.setOption(option1);
    }
    echartsQY1();
    function echartsQY2(){
      var myChart_rqnl = echarts.init(document.getElementById("echarts2"));
      var option_rqnl = {
        color:['#CC9949','#00A6A6'],
        calculable : false,
        grid: {
          y:'5%',
          containLabel: true,
          borderWidth: 0
        },
        legend: {
          data:['男性','女性'],
          show: true,
          y:'bottom',
          textStyle:{
            color:'#fff'
          },
        },
        tooltip : {
          trigger: 'axis',
          formatter: function (params,ticket,callback) {
            var res = '人群年龄分布('+params[0].name+')';
            for (var i = 0, l = params.length; i < l; i++) {
              if(params[i].value<0){
                params[i].value = -params[i].value;
              }
              res += '<br/>' + params[i].seriesName + ' : ' + params[i].value+'%';
            }
            setTimeout(function (){
              // 仅为了模拟异步回调
              callback(ticket, res);
            }, 100)
          }
        },
        xAxis : [
          {
            type : 'value',
            splitLine: {show: false},
            axisLine: {show: false},
            axisTick: {show: false},
            axisLabel: {
              show: true,
              textStyle: text_style,
              formatter:function (value){
                if(value<0){
                  return (-value+"%");
                }else{
                  return (value+'%');
                }
              }
            }
          }
        ],
        yAxis : [
          {
            type: 'category',
            splitLine: {show: false},
            axisLine: {show: false},
            axisLabel: {show: true, textStyle: text_style},
            axisTick : {show: false},
            data : ['0-18岁','18-30岁','30-50岁','50-65岁','65+岁']
          }
        ],
        series : [

          {
            name:'男性',
            type:'bar',
            stack: '总量',
            barWidth : 15,
            itemStyle: {normal: {
              label : {show: false
              }
            }},
            data: [-4, -18, -11, -15, -10]

          },
          {
            name:'女性',
            type:'bar',
            stack: '总量',
            barWidth : 15,
            data:[3, 15, 7, 10, 7]
          }
        ]
      };
      myChart_rqnl.setOption(option_rqnl);
    }
    echartsQY2();
    //echarts--结束
    //高德地图---开始
    function addPoint(arr,h,w){//arr =[经度,纬度]；h=图片高度，w=图片宽	度；img=图片路径
      map.clearMap();
      var _marker=new AMap.Marker({
        map: map,
        position: arr,        //经纬度
        icon: new AMap.Icon({
          size: new AMap.Size(h, w),  //图标大小
          image: "../../images/information/hlwcyhx2.png"       //自定义图片
        })
      });
      map.setCenter(arr);
      AMap.event.addListener(_marker, 'click', function (e) {
//        infoWindow(details)
        //infoWindow.open(map, _marker.getPosition());
      });
    }
    //高德地图---结束
    function returnMainPage(){
      top.location.href="../../shbz.html";
    }

  </script>
</body>
</html>
